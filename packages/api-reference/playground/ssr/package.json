    "sirv": "^3.0.1",
    "vue": "catalog:*"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/node": "catalog:*",
    "@vitejs/plugin-vue": "catalog:*",
    "cross-env": "^7.0.3",
    "vite": "catalog:*"
  }
}
