{
  "name": "@scalar/api-client",
  "description": "the open source API testing client",
  "keywords": [
    "api",
    "client",
    "testing",
    "rest",
    "graphql",
    "postman alternative"
  ],
  "version": "0.5.0",
  "author": "Scalar (https://github.com/scalar)",
  "homepage": "https://github.com/scalar/scalar",
  "repository": {
    "type": "git",
    "url": "https://github.com/scalar/scalar.git",
    "directory": "packages/api-client"
  },
  "bugs": {
    "url": "https://github.com/scalar/scalar/issues/new"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build && pnpm types:build && tsc-alias -p tsconfig.build.json",
    "dev": "vite",
    "lint:check": "eslint .",
    "lint:fix": "eslint .  --fix",
    "preview": "vite preview",
    "test": "vitest",
    "types:build": "vue-tsc -p tsconfig.build.json",
    "types:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    "development": "./src/index.ts",
    "import": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@headlessui/vue": "1.7.14",
    "@scalar/default-theme": "workspace:*",
    "@scalar/use-codemirror": "workspace:*",
    "@scalar/use-keyboard-event": "workspace:*",
    "@vueuse/core": "10.1.2",
    "axios": "1.4.0",
    "javascript-time-ago": "2.5.9",
    "nanoid": "4.0.2",
    "pretty-bytes": "6.1.0",
    "pretty-ms": "8.0.0",
    "tippy.js": "6.3.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "tsc-alias": "1.8.6",
    "vite": "4.4.8",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vitest": "0.34.1",
    "vue-tsc": "1.8.8"
  },
  "peerDependencies": {
    "vue": "3.3.4"
  }
}
