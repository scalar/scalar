    "codemirror": "6.0.1",
    "javascript-time-ago": "2.5.9",
    "nanoid": "4.0.2",
    "nunjucks": "3.2.4",
    "pretty-bytes": "6.1.0",
    "pretty-ms": "8.0.0",
    "tippy.js": "6.3.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "tsc-alias": "1.8.6",
    "vite": "4.4.8",
    "vitest": "0.34.1",
    "vue-tsc": "1.8.8"
  },
  "peerDependencies": {
    "vue": "3.3.4"
  }
