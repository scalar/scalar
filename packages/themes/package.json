    "vite-plugin-css-injected-by-js": "3.3.0",
    "vite-plugin-static-copy": "0.17.0",
    "vitest": "0.34.4",
    "vue-tsc": "1.8.13"
  },
  "exports": {
    ".": "./dist/index.js",
    "./base.css": "./dist/base.css",
