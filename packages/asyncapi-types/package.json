{
  "name": "@scalar/asyncapi-types",
  "description": "AsyncAPI TypeScript types and TypeBox schemas",
  "license": "MIT",
  "author": "Scalar (https://github.com/scalar)",
  "homepage": "https://github.com/scalar/scalar",
  "bugs": "https://github.com/scalar/scalar/issues/new/choose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalar/scalar.git",
    "directory": "packages/asyncapi-types"
  },
  "keywords": [
    "scalar",
    "asyncapi",
    "typescript",
    "typebox",
    "schemas"
  ],
  "version": "0.0.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "scalar-build-esbuild",
    "lint:check": "scalar-lint-check",
    "lint:fix": "scalar-lint-fix",
    "test": "vitest",
    "types:build": "scalar-types-build",
    "types:check": "scalar-types-check"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schemas/v3.0": {
      "import": "./dist/schemas/v3.0/index.js",
      "types": "./dist/schemas/v3.0/index.d.ts",
      "default": "./dist/schemas/v3.0/index.js"
    },
    "./types/v3.0": {
      "import": "./dist/types/v3.0/index.js",
      "types": "./dist/types/v3.0/index.d.ts",
      "default": "./dist/types/v3.0/index.js"
    }
  },
  "files": [
    "./dist",
    "CHANGELOG.md"
  ],
  "readme": {
    "title": "Scalar AsyncAPI Types",
    "badges": [
      {
        "type": "npm-version"
      },
      {
        "type": "npm-downloads"
      },
      {
        "type": "npm-license"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@scalar/build-tooling": "workspace:*",
    "vite": "catalog:*",
    "type-fest": "catalog:*",
    "vitest": "catalog:*"
  },
  "peerDependencies": {
    "@scalar/typebox": "catalog:*"
  },
  "peerDependenciesMeta": {
    "@scalar/typebox": {
      "optional": true
    }
  }
}
