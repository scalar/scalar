{
  "name": "@scalar/icons",
  "description": "Scalar Icons",
  "license": "MIT",
  "author": "Scalar (https://github.com/scalar)",
  "homepage": "https://github.com/scalar/scalar",
  "bugs": "https://github.com/scalar/scalar/issues/new/choose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalar/scalar.git",
    "directory": "packages/icons"
  },
  "version": "0.5.2",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "scalar-build-vite",
    "format": "scalar-format",
    "format:check": "scalar-format-check",
    "generate:icons": "vite-node ./scripts/generate-icons.ts && pnpm format",
    "lint:check": "scalar-lint-check && pnpm lint:icons",
    "lint:fix": "scalar-lint-fix",
    "lint:icons": "svglint src/library/icons/*.svg --config src/library/.svglintrc.js",
    "test": "vitest",
    "types:build": "scalar-types-build-vue",
    "types:check": "scalar-types-check-vue"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./library": {
      "import": "./dist/library/index.js",
      "require": "./dist/library/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1",
    "@types/node": "catalog:*",
    "chalk": "^5.4.1",
    "vue": "catalog:*"
  },
  "devDependencies": {
    "@scalar/build-tooling": "workspace:*",
    "@vitejs/plugin-vue": "catalog:*",
    "@vue/test-utils": "catalog:*",
    "jsdom": "catalog:*",
    "svglint": "^4.1.2",
    "vite": "catalog:*",
    "vite-svg-loader": "catalog:*",
    "vitest": "catalog:*"
  }
}
