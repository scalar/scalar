{
  "name": "@scalar/swagger-parser",
  "version": "0.1.11",
  "author": "Scalar",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && pnpm types:build && tsc-alias -p tsconfig.build.json",
    "dev": "vite",
    "lint:check": "eslint .",
    "lint:fix": "eslint .  --fix",
    "test": "vitest",
    "types:build": "tsc -p tsconfig.build.json",
    "types:check": "tsc --noEmit --skipLibCheck"
  },
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@apidevtools/swagger-parser": "10.1.0",
    "js-yaml": "4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@types/js-yaml": "4.0.5",
    "@types/node": "20.3.1",
    "@vitejs/plugin-vue": "4.2.3",
    "glob": "10.3.3",
    "openapi-types": "12.1.3",
    "rollup": "3.28.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-polyfill-node": "0.12.0",
    "tsc-alias": "1.8.6",
    "tslib": "^2.6.1",
    "typescript": "*",
    "vite": "4.4.8",
    "vitest": "0.34.1"
  }
}
