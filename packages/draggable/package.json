{
  "name": "@scalar/draggable",
  "description": "A simple vue wrapper around html drag and drop",
  "license": "MIT",
  "author": "Scalar (https://github.com/scalar)",
  "homepage": "https://github.com/scalar/scalar",
  "bugs": "https://github.com/scalar/scalar/issues/new/choose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalar/scalar.git",
    "directory": "packages/draggable"
  },
  "keywords": [
    "vue drag and drop draggable html"
  ],
  "version": "0.1.8",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build && pnpm types:build",
    "dev": "vite",
    "lint:check": "eslint .",
    "lint:fix": "eslint .  --fix",
    "preview": "vite preview",
    "test": "vitest",
    "types:build": "vue-tsc",
    "types:check": "vue-tsc --noEmit --skipLibCheck --composite false"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*.css": "./dist/*.css"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "module": "dist/index.js",
  "dependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.12"
  }
}
