{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/src/env.d.ts", "src/renderer/src/**/*", "src/renderer/src/**/*.vue", "src/preload/*.d.ts"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "moduleResolution": "Bundler",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    }
  }
}
