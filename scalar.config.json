{
  "$schema": "https://cdn.scalar.com/schema/scalar-config-next.json",
  "scalar": "2.0.0",
  "info": {
    "title": "Scalar Documentation",
    "description": "Guides for Scalar, covering your favorite frameworks, languages and use cases."
  },
  "publishOnMerge": true,
  "assetsDir": "documentation/assets",
  "publishPreviews": false,
  "pullRequestComments": false,
  "insertPageTitles": false,
  "siteConfig": {
    "subdomain": "guides-v2",
    "head": {
      "scripts": [
        {
          "path": "documentation/assets/landing.js"
        }
      ],
      "styles": [
        {
          "path": "documentation/assets/styles.css"
        }
      ],
      "meta": [
        {
          "name": "description",
          "content": "Guides for Scalar, covering your favorite frameworks, languages and use cases."
        },
        {
          "property": "og:image",
          "name": "og:image",
          "content": "https://api.scalar.com/cdn/images/IlMnm9Mxy-SabmLxE_i0L/8O99z8O2xzEypGxOzuZYo.png"
        }
      ],
      "links": [
        {
            "rel": "icon",
            "href": "/favicon.png"
          }
      ]
    },
    "logo": {
      "darkMode": "https://cdn.scalar.com/marketing/landing/logo-dark.svg",
      "lightMode": "https://cdn.scalar.com/marketing/landing/logo-light.svg"
    },
    "routing": {
      "redirects": [
        {
          "from": "/scalar/scalar-registry/github-workflows",
          "to": "/registry/github-actions"
        },
        {
          "from": "/scalar/scalar-docs/publish-scalar-projects-using-github-actions",
          "to": "/registry/github-actions"
        },
        {
          "from": "/scalar/introduction",
          "to": "/"
        },
        {
          "from": "/scalar",
          "to": "/"
        },
        {
          "from": "/scalar-api",
          "to": "/scalar-api/description/introduction"
        }
      ]
    }
  },
